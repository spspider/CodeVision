//////////////////////////////////////////////////////////////////////////////
//
// Contains resource definitions at Atmel AT90mega83
//
// File version: $Name: V2.21H V2.21G V2_25A $
//
// The '_..X_' prefix is used by C-SPY as an indication that the label should
// not be displayed in the dissassembly window.
//

//////////////////////////////////////////////////////////////////////////////
// Code (flash) segments
-D_..X_INTVEC_SIZE=0      // 4 bytes * 18 vectors
-D_..X_FLASH_NEND=3FFF    // End of near flash memory
-D_..X_FLASH_END=3FFF     // End of flash memory

//////////////////////////////////////////////////////////////////////////////
// Data (SRAM, external ROM or external NV RAM) memory
-D_..X_SRAM_BASE=60
-D_..X_SRAM_TEND=FF
-D_..X_SRAM_END=45F

-D_..X_EXT_SRAM_BASE=0    // External memory not possible
-D_..X_EXT_SRAM_END=0     // External memory not possible
-D_..X_EXT_CSTACK_BASE=0  // External memory not possible
-D_..X_EXT_CSTACK_END=0   // External memory not possible
-D_..X_EXT_RSTACK_BASE=0  // External memory not possible
-D_..X_EXT_RSTACK_END=0   // External memory not possible
-D_..X_EXT_ROM_BASE=0     // External memory not possible
-D_..X_EXT_ROM_END=0      // External memory not possible
-D_..X_EXT_NV_BASE=0      // External memory not possible
-D_..X_EXT_NV_END=0       // External memory not possible

//////////////////////////////////////////////////////////////////////////////
// Internal EEPROM
-D_..X_EEPROM_END=1FF     // End of eeprom memory
